  <object class="GtkListStore" id="format_model">
    <columns>
      <!-- column-name num -->
      <column type="gint"/>
      <!-- column-name home -->
      <column type="gboolean"/>
    </columns>
    <data>
      <row>
        <col id="0">1</col>
        <col id="1">True</col>
      </row>
      <row>
        <col id="0">2</col>
        <col id="1">False</col>
      </row>
      <row>
        <col id="0">3</col>
        <col id="1">True</col>
      </row>
    </data>
  </object>
  <object class="GtkFileChooserDialog" id="load_dialog">
